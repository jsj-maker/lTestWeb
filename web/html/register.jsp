<%@ page language="java" contentType="text/html; charset=UTF-8"
		 pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>

	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>
		注册栾秀网
	</title>

	<link rel="stylesheet" type="text/css" href="../css/base.css"/>
	<link rel="stylesheet" type="text/css" href="../css/register.css"/>

</head>
<body>

	<div class="content">
		<h2 class="register_wel" style="margin-top: 50px">开启你的生活记录</h2>

		<div class="register_ct">
			<p class="register_title">注册栾秀网</p>
			<div class="dash_divider"></div>

			
			<table class="input_ct" cellspacing="20px">
				<tr >
					
					<td>
						<input class="inputor" type="text" name="" placeholder="用户名">
					</td>
					<td id="username_tip">2~4位字符，支持中文，英文，数字</td>
				</tr>
				<tr >
					<td>
						<input class="inputor" type="email" name="" placeholder="邮箱：">
					</td>
					<td id="email_tip">输入您的邮箱</td>
				</tr>
				<tr >
					<td>
						<input class="inputor" type="text" name="" placeholder="密码：">
					</td>
					<td id="password_tip">6~16位数字,字母，或常用英文字符</td>
				</tr>

				<tr >
					<td>
						<input class="inputor" type="text" name="" placeholder="再输一次密码：">
					</td>
					<td id="repassword_tip">6~16位数字,字母，或常用英文字符</td>
				</tr>


			</table>


			<button style="margin-left:400px;background-color: #f0424c;width: 180px;height: 40px;border: 0;color: white; font-size: 23px;">立即注册</button>


		</div>




	</div>

</body>
</html>